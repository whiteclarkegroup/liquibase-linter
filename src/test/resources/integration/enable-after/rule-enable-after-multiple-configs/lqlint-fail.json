{
    "fail-fast": true,
    "rules": {
        "object-name": [
            {
                "pattern": "^(?!_)[A-Z_0-9]+(?<!_)$",
                "errorMessage": "Object name '%s' name must be uppercase and use '_' separation",
                "enableAfter": "integration/enable-after/rule-enable-after-multiple-configs/object-name-fail-1.xml"
            },
            {
                "pattern": "^POWER.*$",
                "errorMessage": "Object name '%s' name must begin with 'POWER'",
                "enableAfter": "integration/enable-after/rule-enable-after-multiple-configs/object-name-fail-1.xml"
            }
        ]
    }
}
